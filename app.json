{
  "expo": {
    "name": "Alpha beta theta Quiz app",
    "slug": "alphabetathetaquizapp",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/res/image/icon1.png",
    "splash": {
      "image": "./src/res/image/alphabeta.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.self.alphabetatheta"
    },
    "android": {
      "package": "com.self.alphabetatheta"
    },
    "packagerOpts": {
      "config": "metro.config.js"
    }
  }
}
